@Article{White2006,
  Title                    = {{A Direct Lyapunov Approach for a Class of Underactuated Mechanical Systems}},
  Author                   = {White, Warren N. and Foss, Mikil and Guo, Xin},
  Journal                  = {2006 American Control Conference},
  Year                     = {2006},
  Pages                    = {103--110},

  Abstract                 = {A Lyapunov direct method is proposed for a class of underactuated, mechanical systems. The direct method is derived in general for systems having n degrees of freedom of which only m {\&}lt; n are actuated. The applications consist of a class of systems where the elements of the mass/inertia matrix and the gravitational forces/torques are either constants or functions of a single generalized position variable and where n is two and m is one. The time derivative of the candidate Lyapunov function produces a relation that is solved via a matching method. Some of the matching equations consist of linear differential and partial differential equations. It is shown for this class of systems, that the solutions of these linear differential and partial differential equations necessary for assuring asymptotic stability can be evaluated numerically as part of the feedback process. Examples are presented involving an inverted pendulum cart and an inertia wheel pendulum},
  Doi                      = {10.1109/ACC.2006.1655338},
  File                     = {:C$\backslash$:/Users/LabUser/University of Pittsburgh/OneDrive - University of Pittsburgh/Mag µBots References/Underactuated/A Direct Lyapunov Approach for a Class of Underactuated Mechanical Systems.pdf:pdf},
  ISBN                     = {1-4244-0210-7},
  ISSN                     = {07431619},
  Keywords                 = {Mechanical systems/robotics,Nonlinear systems,Stability of nonlinear systems}
}

