@Article{Monaghan2009,
  Title                    = {{Control of triceps surae stimulation based on shank orientation using a uniaxial gyroscope during gait.}},
  Author                   = {Monaghan, C C and van Riel, W J B M and Veltink, P H},
  Journal                  = {Medical \& biological engineering \& computing},
  Year                     = {2009},

  Month                    = nov,
  Number                   = {11},
  Pages                    = {1181--8},
  Volume                   = {47},

  Abstract                 = {This article presents a stimulation control method using a uniaxial gyroscope measuring angular velocity of the shank in the sagittal plane, to control functional electrical stimulation of the triceps surae to improve push-off of stroke subjects during gait. The algorithm is triggered during each swing phase of gait when the angular velocity of the shank is relatively high. Subsequently, the start of the stance phase is detected by a change of sign of the gyroscope signal at approximately the same time as heel strike. Stimulation is triggered when the shank angle reaches a preset value since the beginning of stance. The change of angle is determined by integrating angular velocity from the moment of change of sign. The results show that the real-time reliability of stimulation control was at least 95\% for four of the five stroke subjects tested, two of which were 100\% reliable. For the remaining subject, the reliability was increased from 50\% found during the experiment, to 99\% during offline processing. Our conclusion is that a uniaxial gyroscope on the shank is a simple, more reliable alternative to the heel switch for the purpose of restoring push-off of stroke subjects during gait.},
  Doi                      = {10.1007/s11517-009-0539-8},
  File                     = {:C$\backslash$:/Users/neil/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Monaghan, van Riel, Veltink - 2009 - Control of triceps surae stimulation based on shank orientation using a uniaxial gyroscope during g.pdf:pdf},
  ISSN                     = {1741-0444},
  Keywords                 = {Adult,Algorithms,Electric Stimulation Therapy,Electric Stimulation Therapy: methods,Gait,Gait Disorders, Neurologic,Gait Disorders, Neurologic: etiology,Gait Disorders, Neurologic: physiopathology,Gait Disorders, Neurologic: rehabilitation,Gait: physiology,Humans,Middle Aged,Muscle, Skeletal,Muscle, Skeletal: physiopathology,Orientation,Stroke,Stroke: complications,Stroke: physiopathology,Stroke: rehabilitation},
  Owner                    = {LabUser},
  Pmid                     = {19830470},
  Timestamp                = {2015.12.02},
  Url                      = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=2768796\&tool=pmcentrez\&rendertype=abstract}
}

