@Article{Guo2011,
  Title                    = {{A Comparative Evaluation of Sonomyography, Electromyography, Force, and Wrist Angle in a Discrete Tracking Task}},
  Author                   = {Guo, Jing-Yi and Zheng, Yong-Ping and Kenney, Laurence P.J. and Bowen, Audrey and Howard, David and Canderle, Jiri J.},
  Journal                  = {Ultrasound in Medicine {\&} Biology},
  Year                     = {2011},
  Number                   = {6},
  Pages                    = {884--891},
  Volume                   = {37},

  Abstract                 = {We have previously used the real-time change of muscle thickness detected using ultrasound, namely sonomyography (SMG), for prosthesis motor control purposes. In the present study, we further compared subjects' performance using SMG and surface electromyography (EMG) in a series of discrete tracking tasks, both with and without a concurrent auditory attention task. Sixteen healthy subjects used different signals in a random order to control the cursor on a personal computer screen to cancel the letter â€œEâ€? in a sequence of vertically arranged letters. Subjects' performance was evaluated under isometric contraction and wrist extension using the extensor carpi radiali muscle. The percentage of successfully cancelled Es using SMG decreased by 21 Â± 16{\%} and 17 Â± 11{\%} in isometric contraction and wrist extension tests, respectively, compared with the corresponding performances using force and angle signals. The corresponding reduction recorded by using EMG was 40 Â± 29{\%} and 41 Â± 25{\%}. In addition, there was a significant decrease by using EMG compared with that by SMG (p {\textless} 0.001). The results also demonstrated that there was no significant difference of performances of canceling E between the single and dual tasks by using any of the control signals (p {\textgreater} 0.99). Furthermore, the SMG control provided more consistent performances under the single and dual tasks compared with EMG control.},
  Doi                      = {10.1016/J.ULTRASMEDBIO.2011.03.008},
  File                     = {::},
  ISSN                     = {0301-5629},
  Owner                    = {LabUser},
  Publisher                = {Elsevier},
  Timestamp                = {2019.01.18}
}

@Article{Guo2010,
  Title                    = {{Continuous monitoring of electromyography (EMG), mechanomyography (MMG), sonomyography (SMG) and torque output during ramp and step isometric contractions}},
  Author                   = {Guo, Jing-Yi and Zheng, Yong-Ping and Xie, Hong-Bo and Chen, Xin},
  Journal                  = {Medical Engineering {\&} Physics},
  Year                     = {2010},
  Number                   = {9},
  Pages                    = {1032--1042},
  Volume                   = {32},

  Abstract                 = {In this study we simultaneously collected ultrasound images, EMG, MMG from the rectus femoris (RF) muscle and torque signal from the leg extensor muscle group of nine male subjects (meanÂ±SD, age=30.7Â±.4.9 years; body weight=67.0Â±8.4kg; height=170.4Â±6.9cm) during step, ramp increasing, and decreasing at three different rates (50{\%}, 25{\%} and 17{\%} MVC/s). The muscle architectural parameters extracted from ultrasound imaging, which reflect muscle contractions, were defined as sonomyography (SMG) in this study. The cross-sectional area (CSA) and aspect ratio between muscle width and thickness (width/thickness) were extracted from ultrasound images. The results showed that the CSA of RF muscles decreased by 7.25Â±4.07{\%} when muscle torque output changed from 0{\%} to 90{\%} MVC, and the aspect ratio decreased by 41.66Â±7.96{\%}. The muscle contraction level and SMG data were strongly correlated (R2=0.961, P=0.003, for CSA and R2=0.999, P{\textless}0.001, for width/thickness ratio). The data indicated a significant difference (P{\textless}0.05) in percentage changes for CSA and aspect ratio among step, ramp increasing, and decreasing contractions. The normalized EMG RMS in ramp increasing was 8.25Â±4.00{\%} higher than step (P=0.002). The normalized MMG RMS of step contraction was significantly lower than ramp increasing and decreasing, with averaged differences of 12.22Â±3.37{\%} (P=0.001) and 12.06Â±3.37{\%} (P=0.001), respectively. The results of this study demonstrated that the CSA and aspect ratio, i.e., SMG signals, can provide useful information about muscle contractions. They may therefore complement EMG and MMG for studying muscle activation strategies under different conditions.},
  Doi                      = {10.1016/J.MEDENGPHY.2010.07.004},
  File                     = {::},
  ISSN                     = {1350-4533},
  Owner                    = {LabUser},
  Publisher                = {Elsevier},
  Timestamp                = {2019.01.18}
}

@Article{Guo2000,
  Title                    = {{A new type of underwater fish-like microrobot}},
  Author                   = {Guo, Shuxiang Guo Shuxiang and Sugimoto, K. and Hata, S. and Su, Jianliang Su Jianliang and Oguro, K.},
  Journal                  = {Proceedings. 2000 IEEE/RSJ International Conference on Intelligent Robots and Systems (IROS 2000) (Cat. No.00CH37113)},
  Year                     = {2000},
  Number                   = {1},
  Pages                    = {136--141},
  Volume                   = {2},

  Abstract                 = {It is our purpose to develop an underwater microrobot that has the$\backslash$ncharacteristics of flexibility, driven by a low voltage, good response$\backslash$nand safe body. We have reported an underwater fish-like microrobot with$\backslash$n2 DOF that has the characteristics of flexibility, good response and$\backslash$nsafe body before. In this paper, we propose a new prototype model of an$\backslash$nunderwater microrobot utilizing an ICPF (ionic conducting polymer film)$\backslash$nactuator as the servo actuator to realize swimming motion with 3 DOF. A$\backslash$nbiomimetic fish-like microrobot using an ICPF actuator as a propulsion$\backslash$ntail fin and a buoyancy adjuster for a microrobot swimming structure in$\backslash$nwater or aqueous medium is developed. The overall size of the underwater$\backslash$nmicrorobot prototype shaped as a fish is 45 mm in length, 10 mm in width$\backslash$nand 4 mm in thickness. It has a pair of fins and a floatage adjuster.$\backslash$nCharacteristics of the underwater microrobot are measured by changing$\backslash$nthe frequency and amplitude of input voltage. The experimental results$\backslash$nindicate that the swimming speed of the proposed underwater microrobot$\backslash$ncan be controlled by changing the frequency of input voltage, the moving$\backslash$ndirection (upward or downward) can be controlled by changing the$\backslash$namplitude and the frequency of input voltage},
  Doi                      = {10.1109/IROS.2000.893128},
  File                     = {:C$\backslash$:/Users/LabUser/University of Pittsburgh/OneDrive - University of Pittsburgh/Mag µBots References/Swimming/2003-A New Type of Fish-Like Underwater Microrobot.pdf:pdf},
  ISBN                     = {0-7803-6348-5},
  ISSN                     = {1083-4435}
}

